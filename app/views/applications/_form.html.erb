      <div class="form-group">
         <%= f.label :hard_money, 'Is there a current bridge/hard money loan on the property now?' %><br>
         <%= f.collection_radio_buttons :hard_money, [[true, 'Yes'] ,[false, 'No']], :first, :last %>
       </div>
      <div class="form-group">
         <%= f.label :lender_name, 'If yes, list the name and phone of the current funder' %>
         <%= f.text_field :lender_name, class: "form-control" %>
      </div>
    <div class="form-group">
         <%= f.label :rented, 'Is the property currently rented?' %><br>
         <%= f.collection_radio_buttons :rented, [[true, 'Yes'] ,[false, 'No']], :first, :last %>
       </div>
       
       <!--if rented-->
       <div class="form-group">
         <%= f.label :sectioneight, 'Is it currently rented Section 8?' %><br>
         <%= f.collection_radio_buttons :sectioneight, [[true, 'Yes'] ,[false, 'No']], :first, :last %>
       </div>
       <div class="form-group">
         <%= f.label :mtm, 'Is the lease Month-to-Month (MTM) or Annual?' %><br>
         <%= f.collection_radio_buttons :mtm, [[true, 'MTM'] ,[false, 'Annual']], :first, :last %>
       </div>
       <div class="form-group">
         <%= f.label :management, 'Is there currently a 3rd party management company?' %><br>
         <%= f.collection_radio_buttons :management, [[true, 'Yes'] ,[false, 'No']], :first, :last %>
       </div>
       <div class="form-group">
         <%= f.label :mgmt_fee, 'If yes, what is the annual fee paid the the management company?' %>
         <div class="input-group">
         <div class="input-group-addon">$</div>
         <%= f.text_field :mgmt_fee, class: "form-control" %>
         <div class="input-group-addon">.00</div>
      </div>
      
      <!--if rehab-->
      <div class="form-group">
         <%= f.label :as_is_val, 'Current As-Is Value of the Property' %>
         <div class="input-group">
         <div class="input-group-addon">$</div>
         <%= f.text_field :as_is_val, class: "form-control" %>
         <div class="input-group-addon">.00</div>
      </div>
      <div class="form-group">
         <%= f.label :rehab_funds, 'Rehab Funds needed' %>
         <div class="input-group">
         <div class="input-group-addon">$</div>
         <%= f.text_field :rehab_funds, class: "form-control" %>
         <div class="input-group-addon">.00</div>
      </div>
      <div class="form-group">
         <%= f.label :arv, 'Value of the Property after Rehab' %>
         <div class="input-group">
         <div class="input-group-addon">$</div>
         <%= f.text_field :arv, class: "form-control" %>
         <div class="input-group-addon">.00</div>
      </div>
      <div class="form-group">
         <%= f.label :arv_just, 'How are You determining the ARV?' %>
         <%= f.input :arv_just, label: false,  collection: [['Broker Price Opinion', 1], ['Zillow', 2], ['Previous Appraisal', 3], ['Partnership', 4]], prompt: "Select One..." %>
      </div>
      <%= f.submit :Finish, class:'btn btn-success btn-block' %>
      <% end %>
    </div>
    </div>
 
 
  
  
 

</div>
